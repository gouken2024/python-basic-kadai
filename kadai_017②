class Human:
  def __init__(self,name,age):
    self.name = name
    self.age = age

  def check_adult(self):
    if self.age >= 20:
      print("あなたは大人です")

    else:
      print("あなたは大人ではありません")

human = {}

human[1] = Human("侍太郎",25)
human[2] = Human("侍次郎",23)
human[3] = Human("侍三郎",20)
human[4] = Human("侍四郎",18)

for i in range(1, 5):
  human[i].check_adult()
