class Human:
  def __init__(self,name,age):
    self.name = name
    self.age = age

  def check_adult(self):
    if self.age >= 20:
      print("あなたは大人です")

    else:
      print("あなたは大人ではありません")

human1 = Human("侍太郎",25)
human2 = Human("侍次郎",23)
human3 = Human("侍三郎",20)
human4 = Human("侍四郎",18)

human1.check_adult()
human2.check_adult()
human3.check_adult()
human4.check_adult()
